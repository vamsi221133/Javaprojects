package Pratice;

import java.util.Scanner;

public class Frist {
	static String m ;
	public void method()
	{
		
		Scanner sc =new Scanner(System.in);
		System.out.println("Enter the Symbol:");
		 m=sc.nextLine();
		
		 for(int i=1;i<=5;i++)
		 {
			
			 for(int j=1;j<=i;j++)
			 {
				 System.out.print(m);
			 }
			System.out.println("\n");
		 }
	}
	
	public static void main(String[] args) {
		
         Frist a = new Frist();
         a.method();
		

	}

}
